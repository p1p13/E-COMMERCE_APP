<h1>Sign Up</h1>
<%= form_for @user, url: users_path do |f| %>
  <p>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </p>
  
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  
  <p>
    <%= f.label :password_confirmation, "Confirm Password" %>
    <%= f.password_field :password_confirmation %>
  </p>
  
  <p>
    <%= f.submit "Create Account" %>
  </p>
<% end %>



