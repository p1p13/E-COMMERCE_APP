<h1>Users List</h1>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Contact Number</th>
    <th>Country code</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.contact_number %></td>
      <td><%= user.country_code %></td>
      <td><%= button_to "delete", { controller => :users,:action => 'destroy', :id => user.id }, :method => :delete %></td>
    </tr>
  <% end %>
</table>

